= doctype html
html lang=en
head
  meta charset=utf-8
  title Topic Reader
body
  h1 Topic Reader
  table border=1
    tr
      th ID
      th Message
      th Upvotes
      th Downvotes
    {{ range $topic := . }}
      tr
        td {{ $topic.ID }}
        td {{ $topic.Msg }}
        td {{ $topic.Upvotes }}
        td {{ $topic.Downvotes }}
    {{ end }}
